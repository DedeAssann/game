# kivy.require
#: import randint random
#: import DampedScrollEffect kivy.effects.dampedscroll
#: import StackLayout kivy.uix.stacklayout
#: import NoTransition kivy.uix.screenmanager


#: set l_txt_size dp(25)
#: set L_txt_size dp(25)
#: set h_size dp(30)
#: set H_size dp(40)
#: set _bc (.180, .525, .757, .3)
#: set _Bc (.2, .6, 1, 1)

WindowManager:
    FirstWindow:
    SecondWindow:
    Tutorial:


# The manager

<WindowManager>:

<FirstWindow>:
    name: "first"
    load_widget: load_widget
    first_widget: first_widget
    FirstWidget:
        id: first_widget
    LoadingWidget:
        id: load_widget
    

<FirstWidget@RelativeLayout>:
    stack: stack
    canvas.before:
        Rectangle:
            size: self.size
            source: "kivy_project\Galaxy_project\galaxy_images\planet1.jpg"
    Label:
        text: 
            """
            W e l c o m e !   L e t ' s   k n o w   e a c h   o t h e r  ! ! !
            """
        underline: True
        italic: True
        font_name: "kivy_project/Galaxy_project/fonts/Eurostile.ttf"
        font_size: dp(40)
        size_hint: 1, .1
        pos_hint: {"center_x": .5, "top": .95}
    ScrollView:
        canvas.before:
            Color:
                rgba: .458, .866, .866, 1
            Line:
                width: dp(1.25)
                rectangle: self.x, self.y, self.width, self.height
        pos_hint: {"center_x": .5, "top": .825}
        size_hint: .95, .7
        do_scroll_x: False
        do_scroll_y: True
        always_overscoll: True
        bar_width: dp(5)
        bar_color: 0, 0, 0, 0
        smooth_scroll_end: dp(8)
        scroll_wheel_distance: dp(45)
        StackLayout:
            id: stack
            canvas.before:
                Color:
                    rgba: 0, 0, 0, .2
                Rectangle:
                    size: self.size
                    pos: self.pos
            size_hint: .925, None
            padding: dp(20)
            spacing: dp(10)
            height: self.minimum_height
            orientation: "lr-tb"
            l1: l1
            l2: l2
            l3: l3
            l4: l4
            l5: l5
            l6: l6
            L1:
                id: l1
            L2:
                id: l2
            L3:
                id: l3
            L4:
                id: l4
            L6:
                id: l6
            L5:
                id: l5
    GridLayout:
        cols: 2
        spacing: dp(800)
        padding: dp(5)
        size_hint: .9, .1
        pos_hint: {"center_x": .5, "top": .11}
        Label:
            center_x: True
            pos_hint: {"x": .25, "center_y": .5}
            size_hint: .055, .655
        Button:
            canvas:
                Color:
                    rgba: .458, .866, .866, 1
                Line:
                    width: dp(1)
                    rectangle: self.x, self.y, self.width, self.height
            text: "N  E  X  T".upper()
            color: .458, .866, .866, 1
            background_color: 0, 0, 0, 0
            font_name: "kivy_project/Galaxy_project/fonts/Eurostile.ttf"
            font_size: dp(30)
            pos_hint: {"x": .7, "center_y": .5}
            size_hint: .15, .85
            on_release: root.parent.transit_screen()


<L1@MyCustomRelativeLayout>:
    last_name: last_name
    MyCustomLabel:
        text:'l a s t   n a m e  :'.upper()
    StringInput:
        id: last_name
        text:''
        on_text_validate: root.parent.l2.first_name.focus = True
<L2@MyCustomRelativeLayout>:
    first_name: first_name
    MyCustomLabel:
        text:'f i r s t   n a m e  :'.upper()
    StringInput:
        id: first_name
        text:''
        on_text_validate: root.parent.l3.mail.focus = True
<L3@MyCustomRelativeLayout>:
    mail: mail
    MyCustomLabel:
        text:'E - m a i l  :'.upper()
    TextInput:
        id: mail
        text:''
        canvas.after:
            Color:
                rgba: .458, .866, .866, 1
            Line:
                width: dp(.5)
                rectangle: self.x, self.y, self.width, self.height
        font_name: "kivy_project/Galaxy_project/fonts/Eurostile.ttf"
        font_size: dp(47.5)
        size_hint: .5, .65
        pos_hint: {"x": .55, "top": .75}
        multiline: False
        background_color: (0, 0, 0, 0)
        foreground_color: (1, 1, 1, 1)
        color: .458, .866, .866, 1
        cursor_width: dp(3)
        cursor_height: dp(10)
        cursor_color: 1, 1, 1, 1
        on_text_validate: root.parent.l4.age.focus = True
<L4@MyCustomRelativeLayout>:
    age: age
    MyCustomLabel:
        text:'a g e  :'.upper()
    FloatInput:
        id: age
        text:''
        on_text_validate: root.parent.l6.username.focus = True
<L5@MyCustomRelativeLayout>:
    my_layout: my_layout
    MyCustomLabel:
        text:'L a n g u a g e  :'.upper()
    MyLayout:
        id: my_layout
        pos_hint: {"x": .55, "top": .75}
        size_hint: .5, .65
<L6@MyCustomRelativeLayout>:
    username: username
    MyCustomLabel:
        text: "u s e r n a m e  :".upper()
    TextInput:
        id: username
        text: ""
        canvas.after:
            Color:
                rgba: .458, .866, .866, 1
            Line:
                width: dp(.5)
                rectangle: self.x, self.y, self.width, self.height
        font_name: "kivy_project/Galaxy_project/fonts/Eurostile.ttf"
        font_size: dp(47.5)
        size_hint: .5, .65
        pos_hint: {"x": .55, "top": .75}
        multiline: False
        background_color: (0, 0, 0, 0)
        foreground_color: (1, 1, 1, 1)
        color: .458, .866, .866, 1
        cursor_width: dp(3)
        cursor_height: dp(10)
        cursor_color: 1, 1, 1, 1


<MyLayout>:
    canvas:
        Color:
            rgba: 0, 0, 0, 0
        Rectangle:
            size: self.size

<MyCustomLabel@Label>
    font_name: "kivy_project/Galaxy_project/fonts/Sackers-Gothic-Std-Light.ttf"
    font_size: dp(30)
    pos_hint: {"center_x": .25, "top": .75}
    size_hint: .4, .9
    bold: True

<MyCustomRelativeLayout@RelativeLayout>:
    canvas.before:
        Color:
            rgba: 0, 0, 0, .2
        Rectangle:
            size: self.size
    pos_hint: None, None
    size_hint: 1, None
    

<MyCustomDropDown>:
    padding: dp(20)
    spacing: dp(50)
    Button:
        background_color: 0, 0, 0, .65
        color: .458, .866, .866, 1
        text: "E N G L I S H"
        bold: True
        font_name: "kivy_project/Galaxy_project/fonts/Sackers-Gothic-Std-Light.ttf"
        font_size: dp(35)
        size_hint_y: None
        height: dp(70)
        on_release:
            root.select(self.text)
            app.store.put("Language", name="Language", value=root.LANGUAGES[self.text])
    Button:
        background_color: 0, 0, 0, .65
        color: .458, .866, .866, 1
        text: "F R A N C A I S"
        bold: True
        font_name: "kivy_project/Galaxy_project/fonts/Sackers-Gothic-Std-Light.ttf"
        font_size: dp(35)
        size_hint_y: None
        height: dp(70)
        on_release:
            root.select(self.text)
            app.store.put("Language", name="Language", value=root.LANGUAGES[self.text])
    Button:
        background_color: 0, 0, 0, .65
        color: .458, .866, .866, 1
        text: "E S P A N O L"
        bold: True
        font_name: "kivy_project/Galaxy_project/fonts/Sackers-Gothic-Std-Light.ttf"
        font_size: dp(35)
        size_hint_y: None
        height: dp(70)
        on_release:
            root.select(self.text)
            app.store.put("Language", name="Language", value=root.LANGUAGES[self.text])

