<Tutorial>:
    name: "tuto"
    canvas:
        Rectangle:
            size: self.size
            source: "kivy_project\Galaxy_project\galaxy_images\planet1.jpg"
    tuto_01: tuto_01
    Tutorial01:
        id: tuto_01

<PreTutorial>:
    size_hint: 1, 1
    pos_hint: {"left": .95, "top": .95}
    BoxLayout:
        orientation: "vertical"
        spacing: dp(20)
        padding: dp(35)
        Label:
            text: "W E L C O M E"
            valign: "middle"
            size_hint: 1, .2
            color: .458, .866, .866, 1
            font_size: dp(100)
            font_name: "kivy_project/Galaxy_project/fonts/Eurostile.ttf"
        Label:
            text: "T O   T H E"
            valign: "middle"
            size_hint: 1, .15
            color: .458, .866, .866, 1
            font_size: dp(110)
            font_name: "kivy_project/Galaxy_project/fonts/Eurostile.ttf"
        Label:
            text: "G  A  L  A  X  Y"
            valign: "middle"
            size_hint: 1, .25
            color: .458, .866, .866, 1
            font_size: dp(150)
            font_name: "kivy_project/Galaxy_project/fonts/Eurostile.ttf"
        Label:
            text: "G A M E"
            valign: "middle"
            size_hint: 1, .2
            font_size: dp(110)
            color: .458, .866, .866, 1
            font_name: "kivy_project/Galaxy_project/fonts/Eurostile.ttf"
        Label:
            text: "L e t ' s   B e g i n !"
            valign: "middle"
            size_hint: 1, .2
            font_size: dp(100)
            color: .458, .866, .866, 1


<Tutorial01>:
    size_hint: 1, 1
    pos_hint: {"left": .95, "top": 1}
    padding: dp(100)
    BoxLayout:
        orientation: "vertical"
        spacing_y: dp(75)
        padding: dp(15)
        spacing_x: dp(30)
        Label:
            text:
                "How to play ?"
            valign: "top"
            halign: "justify"
            font_name: "kivy_project/Galaxy_project/fonts/Eurostile.ttf"
            font_size: dp(100)
            color: .458, .866, .866, 1
            underline: True
        Label:
            text: 
                "- To go left, press left arrow or the left side of the screen."
            valign: "center"
            halign: "justify"
            font_name: "kivy_project/Galaxy_project/fonts/Eurostile.ttf"
            text_size: self.width, None
            font_size: dp(56)
            #color: .458, .866, .866, 1
        Label:
            text: 
                "- To go right, press right arrow or the right side of the screen."
            valign: "center"
            halign: "justify"
            font_name: "kivy_project/Galaxy_project/fonts/Eurostile.ttf"
            text_size: self.width, None
            font_size: dp(56)
            #color: .458, .866, .866, 1
        Label:
            text:
                "- But, always remember to keep following the track !"
            valign: "center"
            halign: "justify"
            font_name: "kivy_project/Galaxy_project/fonts/Eurostile.ttf"
            text_size: self.width, None
            font_size: dp(56)
            #color: .458, .866, .866, 1
    GridLayout:
        cols: 2
        spacing: dp(800)
        padding: dp(5)
        size_hint: .9, .1
        pos_hint: {"center_x": .5, "top": .11}
        Label:
            center_x: True
            pos_hint: {"x": .25, "center_y": .5}
            size_hint: .055, .655
        Button:
            canvas.before:
                Color:
                    rgba: 0.458, 0.866, 0.866, 1
                Line:
                    width: dp(1.15)
                    rectangle: self.x, self.y, self.width, self.height
            text: "N  E  X  T"
            #color: 0.458, 0.866, 0.866, 1
            background_color: 0, 0, 0, 0
            bold: True
            font_name: "kivy_project/Galaxy_project/fonts/Eurostile.ttf"
            font_size: dp(37.5)
            pos_hint: {"x": .7, "center_y": .5}
            size_hint: .15, .85        

    
    





